# Copy this file to .env and replace placeholders with your values

# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
MINIO_DEFAULT_BUCKET=docbuilder

# MongoDB Configuration
MONGO_USERNAME=admin
MONGO_PASSWORD=admin
MONGO_DATABASE=docbuilder
MONGO_CONNECTION_STRING=mongodb://admin:admin@localhost:27017
MongoDB__TemplatesCollectionName=templates

# RabbitMQ Configuration
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin

# ASP.NET Core Configuration
ASPNETCORE_ENVIRONMENT=Development
